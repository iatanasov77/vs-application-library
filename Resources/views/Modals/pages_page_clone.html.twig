<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">{{ 'vs_cms.modal.page_clone.title' | trans( {},'VSCmsBundle' ) }}</h5>
            <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            {{ form_start(formClone, { 'method': 'POST', 'attr': {'class': 'form-horizontal', 'id': 'formClone'} }) }}
                <div class="form-group">
                    {{ form_label(formClone.newTitle, null, {
                        'label_attr': {'class': 'col-form-label'}
                        }) 
                    }}
                    {{ form_widget(formClone.newTitle, {
                        'attr': {
                            'class': 'form-control',
                        }
                    }) }}
                </div>
            {{ form_end(formClone, {'render_rest': false}) }}
        </div>
        <div class="modal-footer" id="imContainer">
            <button type="button" class="btn btn-primary" id="btnSaveFormClone">
				{{ 'vs_cms.modal.page_clone.button_clone' | trans( {},'VSCmsBundle' ) }}
			</button>
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
				{{ 'vs_application.form.cancel' | trans( {},'VSApplicationBundle' ) }}
			</button>
        </div>
    </div>
</div>
