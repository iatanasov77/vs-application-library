{
	"name" : "vankosoft/application-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony VSApplicationBundle",
	"keywords" : [
		"Symfony Application"
	],
	"homepage" : "https://github.com/iatanasov77/vs-application-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ivan I. Atanasov",
			"email" : "i.atanasov77@gmail.com"
		}
	],
	"require" : {
		"php" : "^8.2",
		"symfony/monolog-bundle" : "^4.0",
		"symfony/webpack-encore-bundle" : "^2.4",
		"symfony/templating" : "^6.4 || ^7.4",
		"symfony/http-client" : "^6.4 || ^7.4",
		"twig/extra-bundle" : "^3.22",
		"twig/string-extra" : "^3.22",
		"twig/intl-extra" : "^3.22",
		"doctrine/doctrine-migrations-bundle" : "^3.7",
		"stof/doctrine-extensions-bundle" : "^1.14",
		"doctrine/common": "^3.5",
		"knplabs/knp-menu-bundle" : "^3.7",
		"knplabs/knp-paginator-bundle" : "^6.10",
		"knplabs/knp-time-bundle" : "^2.5",
		"sylius-labs/doctrine-migrations-extra-bundle" : "^0.2",
		"sylius/resource-bundle" : "^1.13",
		"sylius/taxonomy" : "^2.2",
		"sylius/locale" : "^2.2",
		"sylius/theme-bundle" : "^2.4",
		"friendsofsymfony/jsrouting-bundle" : "^3.6",
		"willdurand/js-translation-bundle" : "^7.0",
		"hackzilla/password-generator-bundle" : "^6.0",
		"vankosoft/agent-bundle" : "^0.1"
	},
	"suggest" : {
		"symfony/debug-bundle" : "Allows developement debugging",
		"symfony/maker-bundle" : "Standard symfony console maker tools",
		"symplify/changelog-linker" : "Allows generation of nice CHANGELOG.md file"
	},
	"conflict" : {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload" : {
		"psr-4" : {
			"Vankosoft\\ApplicationBundle\\" : "/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../*"
		}
	],
	"minimum-stability" : "dev"
}