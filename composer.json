{
	"name" : "vankosoft/cms-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony CMS Bundle",
	"keywords" : [
		"CMS",
		"HTML",
		"Editor",
		"Image Manipulation"
	],
	"homepage" : "https://github.com/iatanasov77/vs-cms-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ivan I. Atanasov",
			"email" : "i.atanasov77@gmail.com"
		}
	],
	"require" : {
		"php" : "^8.1",
		"vankosoft/application-bundle" : "^1.10",
		"friendsofsymfony/ckeditor-bundle" : "^2.5",
        "daddl3/symfony-ckeditor-5-webpack": "^1.2",
		"liip/imagine-bundle" : "^2.12",
		"artgris/filemanager-bundle" : "^2.10",
		"knplabs/knp-gaufrette-bundle" : "^0.9",
		"oneup/uploader-bundle" : "^4.0",
		"oneup/flysystem-bundle" : "^4.11"
	},
	"conflict" : {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload" : {
		"psr-4" : {
			"Vankosoft\\CmsBundle\\" : "lib/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../*"
		}
	],
	"minimum-stability" : "dev"
}