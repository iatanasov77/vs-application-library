{
    "name": "vankosoft\/application",
    "type": "library",
    "version": "v1.12.30",
    "description": "VankoSoft Symfony Application Core Library",
    "keywords": [
        "symfony",
        "sylius",
        "application",
        "distribution"
    ],
    "homepage": "https:\/\/github.com\/iatanasov77\/vs-application-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan I. Atanasov",
            "email": "i.atanasov77@gmail.com",
            "homepage": "http:\/\/vankosoft.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony\/monolog-bundle": "^3.8",
        "symfony\/webpack-encore-bundle": "^2.2",
        "symfony\/templating": "^6.4",
        "symfony\/security-bundle": "^6.4",
        "symfony\/mailer": "^6.4",
        "symfony\/http-client": "^6.4",
        "twig\/extra-bundle": "^3.3",
        "twig\/string-extra": "^3.3",
        "twig\/intl-extra": "^3.5",
        "symfonycasts\/verify-email-bundle": "^1.16",
        "symfonycasts\/reset-password-bundle": "1.20.*",
        "doctrine\/doctrine-migrations-bundle": "^3.3",
        "stof\/doctrine-extensions-bundle": "^1.11",
        "doctrine\/doctrine-fixtures-bundle": "^3.5",
        "knplabs\/knp-menu-bundle": "^3.3",
        "knplabs\/knp-paginator-bundle": "^6.3",
        "knplabs\/knp-time-bundle": "^2.2",
        "friendsofsymfony\/ckeditor-bundle": "^2.6",
        "daddl3\/symfony-ckeditor-5-webpack": "^1.2",
        "liip\/imagine-bundle": "^2.13",
        "artgris\/filemanager-bundle": "^2.10",
        "sylius\/resource-bundle": "^1.12",
        "sylius\/taxonomy": "^2.0",
        "sylius\/locale": "^2.0",
        "sylius\/theme-bundle": "^2.4",
        "sylius-labs\/doctrine-migrations-extra-bundle": "^0.2",
        "sylius\/fixtures-bundle": "^1.9",
        "friendsofsymfony\/jsrouting-bundle": "^3.5",
        "willdurand\/js-translation-bundle": "^6.0",
        "hackzilla\/password-generator-bundle": "^6.0",
        "pagerfanta\/doctrine-orm-adapter": "^4.3",
        "pagerfanta\/twig": "^4.3",
        "oneup\/uploader-bundle": "^5.0",
        "oneup\/flysystem-bundle": "^4.12",
        "league\/flysystem-ftp": "^3.29",
        "league\/flysystem-sftp-v3": "^3.29",
        "m2mtech\/flysystem-stream-wrapper": "^1.4",
        "vankosoft\/agent-bundle": "^1.0"
    },
    "replace": {
        "vankosoft\/application-bundle": "self.version",
        "vankosoft\/application-instalator-bundle": "self.version",
        "vankosoft\/users-bundle": "self.version",
        "vankosoft\/cms-bundle": "self.version"
    },
    "suggest": {
        "symfony\/debug-bundle": "Allows developement debugging",
        "symfony\/maker-bundle": "Standard symfony console maker tools",
        "symplify\/changelog-linker": "Allows generation of nice CHANGELOG.md file"
    },
    "conflict": {
        "doctrine\/doctrine-bundle": "<1.3",
        "symfony\/doctrine-bridge": "<2.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "autoload": {
        "psr-4": {
            "Vankosoft\\ApplicationInstalatorBundle\\": "src\/Vankosoft\/ApplicationInstalatorBundle\/",
            "Vankosoft\\ApplicationBundle\\": "src\/Vankosoft\/ApplicationBundle\/",
            "Vankosoft\\UsersBundle\\": "src\/Vankosoft\/UsersBundle\/",
            "Vankosoft\\CmsBundle\\": "src\/Vankosoft\/CmsBundle\/"
        }
    }
}