{{ form_start(form, { 'action': path( 'vs_cms_filemanager_vankosoft_handle_upload_form' ), 'method': 'POST', 'attr': {'class': 'form-horizontal', 'id': 'formUploadFile'} }) }}
    {{ form_widget(form.fileManagerId, {'value' : fileManagerId}) }}
    
    <div class="form-group mb-3">
        <label for="game_form_picture" class="col-form-label">
            {{ 'vs_cms.form.filemanager.file_lable'  | trans( {},'VSCmsBundle' ) }}
        </label>
        <div class="form-field-file">
            
            {{ form_widget(form.file, {
                'attr': {
                    'class': 'form-control',
                }
            }) }}
            {{ form_label(form.file, null, {
                'label_attr': {'class': 'input-group-text'}
                }) 
            }}
        </div>
    </div>
    <div class="mb-3">&nbsp;</div>
{{ form_end(form, {'render_rest': false}) }}